{
  "name": "mimdb",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    "Becca",
    "Wayne",
    "Jiaqi",
    "Nick M.",
    "Katie K.",
    "Alex",
    "Nicholas S."
  ],
  "description": "Middlebury Movie Database (Mimdb) is a project for CS312-Software Development that aims to provide a platform for films created by students and professors in Middlebury film department",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "lint": "eslint src"
  },
  "dependencies": {
    "fuse": "^0.0.8",
    "fuse.js": "^6.4.6",
    "knex": "^0.95.13",
    "next": "11.1.2",
    "next-connect": "^0.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "babel-jest": "^27.2.5",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,jsx}": "prettier --write"
  },
  "repository": {
    "url": "https://github.com/csci0312-f21/project-mimdb.git"
  }
}
