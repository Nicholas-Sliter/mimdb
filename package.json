{
  "name": "mimdb",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    "Becca",
    "Wayne",
    "Jiaqi",
    "Nick M.",
    "Katie K.",
    "Alex",
    "Nicholas S."
  ],
  "description": "Middlebury Movie Database (Mimdb) is a project for CS312-Software Development that aims to provide a platform for films created by students and professors in Middlebury film department",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "lint": "eslint src"
  },
  "dependencies": {
    "fuse": "^0.0.8",
    "fuse.js": "^6.4.6",
    "knex": "^0.95.14",
    "next": "11.1.2",
    "next-connect": "^0.11.0",
    "pg": "^8.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-player": "^2.9.0",
    "react-select": "^5.2.1",
    "sass": "^1.43.4",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.36",
    "babel-jest": "^27.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,jsx}": "prettier --write"
  },
  "repository": {
    "url": "https://github.com/csci0312-f21/project-mimdb.git"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
